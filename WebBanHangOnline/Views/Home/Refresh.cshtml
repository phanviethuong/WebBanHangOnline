@model WebBanHangOnline.Models.ThongKeModel
@if(Model != null)
 {
    <div class="thongke">
        <table class="table">
            <tr>
                <td>Hôm nay</td>
                <td>@Model.HomNay</td>
            </tr>
            <tr>
                <td>Hôm qua</td>
                <td>@Model.HomQua</td>
            </tr>
            <tr>
                <td>Tuần này</td>
                <td>@Model.TuanNay</td>
            </tr>
            <tr>
                <td>Tuần trước</td>
                <td>@Model.TuanTruoc</td>
            </tr>
            <tr>
                <td>Tháng này</td>
                <td>@Model.ThangNay</td>
            </tr>
            <tr>
                <td>Tháng trước</td>
                <td>@Model.ThangTruoc</td>
            </tr>
        </table>
    </div>
 }
